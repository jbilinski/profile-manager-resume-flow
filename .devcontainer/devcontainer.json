{
  "name": "Resume Flow",
  "dockerComposeFile": "compose.yaml",
  "service": "resume-flow",
  "workspaceFolder": "/workspace",
  "customizations": {
    "codespaces": {
      "openFiles": [
        "web_app.py"
      ]
    },
    "vscode": {
      "settings": {},
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]
    }
  },
  "portsAttributes": {
    "8501": {
      "label": "Application",
      "onAutoForward": "openPreview"
    }
  },
  "forwardPorts": [
    8501
  ]
}